{
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "rust-analyzer.linkedProjects": [
    ".\\Cargo.toml"
  ],
  "rust-analyzer.showUnlinkedFileNotification": false,
  "json.schemas": [
    {
      "fileMatch": [
        "package.json",
        ".eemojirc",
        ".eemojirc.json",
        ".eemojirc.yaml",
        ".eemojirc.yml",
        ".config/eemojirc",
        ".config/eemojirc.json",
        ".config/eemojirc.yaml",
        ".config/eemojirc.yml"
      ],
      "url": "https://rettend.github.io/eemoji/eemoji-config-schema.json"
    }
  ]
}